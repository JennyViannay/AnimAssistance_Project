<div class="form-widget">
    {% for image in field.value %}
        <a href="#" class="ea-lightbox-thumbnail" data-feartherlight="#ea-lightbox-{{image.id}}"
            data-feartherlight-close-on-click="anywhere">
            <img src="/uploads/images/{{image.image}}" alt="{{ image.image }}" style="max-width: 150px">
        </a>
        <div id="ea-lightbox-{{image.id}}" class="ea-lightbox">
            <img src="/uploads/images/{{image.image}}" alt="{{ image.image }}" style="max-width: 150px">
        </div>
    {% endfor %}
</div>